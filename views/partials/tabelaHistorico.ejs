<table border="1">
    <tr>
        <th>Data</th>
        <th>Nome</th>
        <th>Ação-Quantidade-Alimento-Capacidade-Validade</th>
        <% if (tipo === "admin") { %>      
            <th>Apagar</th>
        <% } %> 
    </tr>

    <% if(historico.length !=0){ 
        historico.forEach(function(h){ %>
        <tr>
            <td>
                <%= h.dataonly + " " + h.timeonly; %>
            </td>
            <td>
                <%= h.nome %>
            </td>
            <td>
                <%= h.acao %>
            </td>
            <% if (tipo === "admin") { %>      
                    <td><a href="/admin/delete/historico/<%=h.id%>">Apagar</a></td>
            <% } %>
        </tr>
        <% }) %>
    <% } else { %>
        <tr>
            <td colspan="7">Sem histórico</td>
        </tr>
    <% } %>
</table>